# Host github_company
#     Hostname github.com
#     User git
#     IPreferredAuthentications publickey
#     IdentityFile ~/.ssh/id_rsa_companyacc

# gitlab account
Host gitlab
    Hostname gitlab.com
    User git
    PreferredAuthentications publickey
    IdentityFile ~/.ssh/id_ed25519.pub
